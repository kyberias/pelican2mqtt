{
  "mqtt": {
    "broker": "homeassistant",
    //"username": "YourUsername",
    //"password": "123456",
    "baseTopic": "pelicanAC"
  },
  "serialPort": "COM3",
  //"deviceSerialNumber": "P16192",
  "homeAssistantAutoDiscovery": true,
  "registers": [
    {
      "address": 71,
      "index": 0,
      "type": "Celsius",
      "topic": "temperatures/room",
      "name": "Room temperature",
      "autoDiscovery": true
    },
    {
      "address": "0xE6",
      "index": 0,
      "type": "Celsius",
      "topic": "temperatures/supplyAfterHeatRecoveryAndHeatingAndCooling",
      "name": "Supply temperature after heat recovery and heating and cooling",
      "autoDiscovery": true
    },
    {
      "address": "0xE6",
      "index": 1,
      "type": "Celsius",
      "topic": "temperatures/exhaust",
      "name": "Exhaust temperature",
      "autoDiscovery": true
    },
    {
      "address": "0xE6",
      "index": 2,
      "type": "Celsius",
      "topic": "temperatures/waste",
      "name": "Waste temperature",
      "autoDiscovery": true
    },
    {
      "address": "0xE6",
      "index": 3,
      "type": "Celsius",
      "topic": "temperatures/outside",
      "name": "Ulkolämpötila",
      "autoDiscovery": true
    },
    {
      "address": "0xE6",
      "index": 4,
      "type": "Celsius",
      "topic": "temperatures/returnWater",
      "name": "Return water temperature"
    },
    {
      "address": "0xE6",
      "index": 5,
      "type": "Celsius",
      "topic": "temperatures/supplyAfterHeatRecovery",
      "name": "Supply temperature after heat recovery",
      "autoDiscovery": true
    },
    {
      "address": "0xE7",
      "index": "0",
      "type": "Celsius",
      "topic": "settings/temperatures/supply",
      "name": "Supply temperature setting"
    },
    {
      "address": "0xF1",
      "index": "1",
      "type": "Percentage",
      "topic": "heatRecovery/efficiency",
      "name": "Heat recovery efficiency",
      "autoDiscovery": true
    },
    {
      "address": "0xF1",
      "index": "3",
      "type": "PercentageOfMaximum",
      "topic": "cooling/actuator",
      "name": "Cooling actuator control",
      "autoDiscovery": true
    },
    {
      "address": "0xEA",
      "index": "3",
      "type": "RelativeHumidity",
      "topic": "humidity/roomRelativeHumidity",
      "name": "Relative room humidity",
      "autoDiscovery": true
    },
    {
      "address": "0xF0",
      "index": "1",
      "type": "VentilationSpeed",
      "topic": "fans/supply/speed",
      "name": "Supply fan speed",
      "autoDiscovery": true
    },
    {
      "address": "0xF0",
      "index": "2",
      "type": "VentilationSpeed",
      "topic": "fans/exhaust/speed",
      "name": "Exhaust fan speed",
      "autoDiscovery": true
    },
    {
      "address": "0xF0",
      "index": 5,
      "type": "Bitfield",
      "bits": [
        {
          "name": "Filter change 4-month reminder",
          "topic": "alerts/filterReminder",
          "bit": 0,
          "type": "Problem",
          "autoDiscovery": true
        }
      ]
    },
    {
      "address": "0xF1",
      "index": "0",
      "type": "Bitfield",
      "bits": [
        {
          "name": "Heat recovery wheel status",
          "topic": "hrw/rotation",
          "bit": 2,
          "type": "HrwOnOff",
          "autoDiscovery": true
        },
        {
          "name": "Heater status",
          "topic": "heater/status",
          "bit": 3,
          "type": "HeatOnOff",
          "autoDiscovery": true
        },
        {
          "name": "Overpressure status",
          "topic": "overPressure/status",
          "bit": 4
        }
      ]
    },
    {
      "address": "0xF2",
      "index": "3",
      "type": "Bitfield",
      "bits": [
        {
          "name": "Heating or cooling status",
          "topic": "heaterOrCoolingStatus",
          "bit": 0
        },
        {
          "name": "Cooker hood status",
          "topic": "cookerHood/status",
          "bit": 2
        },
        {
          "name": "Central vacuum cleaner status",
          "topic": "centralVacuumCleaner/status",
          "bit": 3
        }
      ]
    }
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  }
}
